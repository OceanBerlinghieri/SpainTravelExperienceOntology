    <!-- http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/meanPrice -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/meanPrice">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Represents de mean price of a given entity</rdfs:comment>
        <rdfs:label>meanPrice</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/nightPrice -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/nightPrice">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Represents the price per night of a service such as a Hotel</rdfs:comment>
        <rdfs:label>nightPrice</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/rating -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/rating">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Represents the rating of a given entity such as a Hotel or a Restaurant</rdfs:comment>
        <rdfs:label>rating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/stars -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/stars">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Represents the stars of a Hotel</rdfs:comment>
        <rdfs:label>stars</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://schema.org/latitude -->

    <owl:DatatypeProperty rdf:about="https://schema.org/latitude">
        <rdfs:domain rdf:resource="https://schema.org/City"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Represents the latitude coordinate of a given entity</rdfs:comment>
        <rdfs:isDefinedBy>https://schema.org/latitude</rdfs:isDefinedBy>
        <rdfs:label>latitude</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://schema.org/longitude -->

    <owl:DatatypeProperty rdf:about="https://schema.org/longitude">
        <rdfs:domain rdf:resource="https://schema.org/City"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Represents the longitude coordinate of a given entity</rdfs:comment>
        <rdfs:isDefinedBy>https://schema.org/longitude</rdfs:isDefinedBy>
        <rdfs:label>longitude</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://schema.org/startDate -->

    <owl:DatatypeProperty rdf:about="https://schema.org/startDate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://schema.org/Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Represents the start date of an event</rdfs:comment>
        <rdfs:isDefinedBy>https://schema.org/startDate</rdfs:isDefinedBy>
        <rdfs:label>startDate</rdfs:label>
    </owl:DatatypeProperty>
    


