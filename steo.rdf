    <!-- http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/servesCuisine -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/servesCuisine">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/Restaurant"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/Cuisine"/>
        <rdfs:comment>Defines the cuisine type such as italian cuisine that a restaurant serves</rdfs:comment>
        <rdfs:label>servesCuisine</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://dbpedia.org/ontology/locationCity -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/locationCity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="https://schema.org/City"/>
        <rdfs:comment>Defines where an entity such as an event is located. This can be a city, country, etc.</rdfs:comment>
        <rdfs:isDefinedBy>https://dbpedia.org/ontology/locationCity</rdfs:isDefinedBy>
        <rdfs:label>locationCity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/isOfferedBy -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isOfferedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="https://saref.etsi.org/core/offers"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/Transport"/>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/Company"/>
        <rdfs:comment>Defines a relation where an entity such a service is offered by other entities such as a company</rdfs:comment>
        <rdfs:isDefinedBy>https://saref.etsi.org/core/isOfferedBy</rdfs:isDefinedBy>
        <rdfs:label>isOfferedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/offers -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/offers">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/TransportCompany"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberlinghieri/ontologies/2025/steo/Transport"/>
        <rdfs:comment>Defines a relation where an entity such as a company offers a service such as a transport service</rdfs:comment>
        <rdfs:isDefinedBy>https://saref.etsi.org/core/offers</rdfs:isDefinedBy>
        <rdfs:label>offers</rdfs:label>
    </owl:ObjectProperty>
    


